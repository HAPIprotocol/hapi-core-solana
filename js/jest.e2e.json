{
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js",
    "json"
  ],
  "transform": {
    "^.+\\.tsx?$": "ts-jest"
  },
  "testRegex": "/test/.*\\.(test-e2e|spec-e2e).(ts|tsx|js)$",
  "collectCoverageFrom": [
    "src/**/*.{js,jsx,tsx,ts}",
    "!**/node_modules/**"
  ],
  "coverageReporters": [
    "json",
    "lcov"
  ],
  "testEnvironment": "node",
  "globalSetup": "./test/setup.ts",
  "globalTeardown": "./test/teardown.ts"
}